{
	"name": "movement",
	"main": "expo-router/entry",
	"version": "1.0.0",
	"scripts": {
		"start": "expo start",
		"start:clean": "expo start --clear",
		"start:dev": "expo start --dev-client",
		"start:tunnel": "expo start --tunnel",
		"reset-project": "node ./scripts/reset-project.js",
		"prebuild": "expo prebuild",
		"prebuild:ios": "expo prebuild --platform ios",
		"prebuild:android": "expo prebuild --platform android",
		"prebuild:clean": "expo prebuild --clean",
		"prebuild:clean:ios": "expo prebuild --platform ios --clean",
		"prebuild:clean:android": "expo prebuild --platform android --clean",
		"android": "expo run:android",
		"android:release": "expo run:android --variant release",
		"android:clean": "cd android && ./gradlew clean && cd ..",
		"ios": "expo run:ios",
		"ios:device": "expo run:ios --device",
		"ios:release": "expo run:ios --configuration Release",
		"ios:clean": "cd ios && xcodebuild clean && cd ..",
		"web": "expo start --web",
		"test": "jest --watchAll",
		"lint": "eslint . --ext .js,.jsx,.ts,.tsx",
		"lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
		"format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
		"format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
		"clean": "rm -rf node_modules && rm -rf ios/build && rm -rf android/build && rm -rf android/app/build",
		"clean:all": "yarn clean && rm -rf ios/Pods && rm -rf ios/Podfile.lock && yarn cache clean",
		"reinstall": "yarn clean:all && yarn install && cd ios && pod install && cd ..",
		"postinstall": "patch-package",
		"prepare": "husky"
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx}": [
			"eslint --fix",
			"prettier --write"
		],
		"*.{json,css,md}": [
			"prettier --write"
		]
	},
	"jest": {
		"preset": "jest-expo"
	},
	"dependencies": {
		"@expo/vector-icons": "^14.0.2",
		"@react-navigation/bottom-tabs": "^7.2.0",
		"@react-navigation/native": "^7.0.14",
		"@shopify/react-native-skia": "1.5.0",
		"expo": "~52.0.48",
		"expo-blur": "~14.0.3",
		"expo-constants": "~17.0.8",
		"expo-font": "~13.0.4",
		"expo-haptics": "~14.0.1",
		"expo-linking": "~7.0.5",
		"expo-router": "~4.0.22",
		"expo-localization": "~17.0.8",
		"expo-splash-screen": "~0.29.24",
		"expo-status-bar": "~2.0.1",
		"expo-symbols": "~0.2.2",
		"expo-system-ui": "~4.0.9",
		"expo-web-browser": "~14.0.2",
		"patch-package": "^8.0.0",
		"postinstall-postinstall": "^2.1.0",
		"react": "18.3.1",
		"react-dom": "18.3.1",
		"react-native": "0.76.9",
		"react-native-fast-confetti": "^1.1.1",
		"react-native-gesture-handler": "~2.20.2",
		"react-native-reanimated": "3.16.1",
		"react-native-safe-area-context": "4.12.0",
		"react-native-screens": "~4.4.0",
		"react-native-vision-camera": "4.7.3",
		"react-native-web": "~0.19.13",
		"react-native-webview": "13.12.5",
		"react-native-worklets-core": "1.6.2",
		"i18next": "^23.11.5",
		"react-i18next": "^15.1.4",
		"zustand": "^5.0.3",
		"@react-native-async-storage/async-storage": "^2.1.1"
	},
	"devDependencies": {
		"@babel/core": "^7.25.2",
		"@types/jest": "^29.5.12",
		"@types/react": "~18.3.12",
		"@types/react-test-renderer": "^18.3.0",
		"@typescript-eslint/eslint-plugin": "^7.0.0",
		"@typescript-eslint/parser": "^7.0.0",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.1.3",
		"eslint-plugin-react": "^7.33.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"husky": "^9.1.7",
		"jest": "^29.2.1",
		"jest-expo": "~52.0.6",
		"lint-staged": "^16.2.7",
		"prettier": "^3.2.5",
		"reactotron-react-native": "^5.1.7",
		"react-test-renderer": "18.3.1",
		"typescript": "^5.3.3"
	},
	"private": true,
	"packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
